# Bucles conocemos el numero de repeticiones
def bucle_1:
    suma=0 #es una veriable acumuladora
    for cont in range(1,11):
      num=int(input("Dime un número: "))
      suma=num+suma
    print(suma)
    
bucle_1()


# Tabla del 3
def tabla_multiplicar():
    tabla=input("Dime la tabla: ")
    suma=0 #es una veriable acumuladora
    for cont in range(1,11):
      print(tabla + " x "+cont +" = "
      
